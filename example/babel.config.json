{
    "plugins": [["react-css-modules", {
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss",
            "plugins": ["postcss-nested"]
          }
        },
        "exclude": "node_modules",
        "handleMissingStyleName": "warn",
        "generateScopedName": "[name]__[local]___[hash:base64:5]"
      }]]
}